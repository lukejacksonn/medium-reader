{
	"name": "Medium Reader",
	"version": "1.0.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["https://medium.com/*"],
			"js": [
				"jquery.min.js",
				"main.js"
			],
			"css": [
				"css/styles.css"
			]
		}
	],
	"icons": {
		"16":  "images/16x16.png",
		"48":  "images/48x48.png",
		"128": "images/128x128.png"
	},
	"web_accessible_resources": ["images/xmas/38x38.png"]
}
